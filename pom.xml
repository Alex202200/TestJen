<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.alex</groupId>
  <artifactId>TestWEB</artifactId>
  <packaging>war</packaging>
  <properties>
    <maven.compiler.source>17</maven.compiler.source> <!-- Обновлено до версии 1.9 -->
    <maven.compiler.target>17</maven.compiler.target> <!-- Обновлено до версии 1.9 -->
  </properties>
  <version>1.0-SNAPSHOT</version>
  <name>TestWEB Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <finalName>TestWEB</finalName>
    <plugins> <!-- Добавлен раздел plugins -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-war-plugin</artifactId>
        <version>3.3.2</version> <!-- Укажите здесь последнюю версию -->
      </plugin>
    </plugins>
  </build>
</project>
